<?xml version="1.0" encoding="UTF-8"?>
<sparqlMap namespace="association"
	xmlns="http://mayo.edu/twinkql" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://mayo.edu/twinkql ../../../../../../../main/castor/twinkql.xsd ">
	
	<resultMap id="whatGovernorsAreShortResultMap" resultClass="edu.mayo.twinkql.it.association.Governor">
		<uniqueResult>s</uniqueResult>
		<rowMap  var="s" varType="localName" beanProperty="name" />
		<rowMap var="isa" varType="localName" beanProperty="isA[]"/>
	</resultMap>
	
	<resultMap id="whatGovernorsAreObjectResultMap" resultClass="edu.mayo.twinkql.it.association.Governor">
		<uniqueResult>s</uniqueResult>
		<rowMap  var="s" varType="localName" beanProperty="name" />
		<association beanProperty="isAObject" isCollection="true" resultClass="edu.mayo.twinkql.it.association.IsA">
			<rowMap var="isa" varType="localName" beanProperty="isA"/>
		</association>
	</resultMap>
	
	<select id="whatGovernorsAreShort" resultMap="whatGovernorsAreShortResultMap">

	SELECT DISTINCT ?s ?isa 
	WHERE {
	
	<![CDATA[
	?s a <http://dbpedia.org/ontology/Governor> ;
	<http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?isa
	]]>
	
	} LIMIT 100
	
	</select>
	
	<select id="whatGovernorsAreObject" resultMap="whatGovernorsAreObjectResultMap">

	SELECT DISTINCT ?s ?isa 
	WHERE {
	
	<![CDATA[
	?s a <http://dbpedia.org/ontology/Governor> ;
	<http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ?isa
	]]>
	
	} LIMIT 100
	
	</select>

</sparqlMap>