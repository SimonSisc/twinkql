<?xml version="1.0" encoding="UTF-8"?>
<sparqlMap namespace="reasoning"
	xmlns="http://mayo.edu/twinkql" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://mayo.edu/twinkql https://raw.github.com/cts2/twinkql/master/src/main/castor/twinkql.xsd">
	
	<perRowResultMap id="reasoningResultMap" resultClass="edu.mayo.twinkql.result.beans.reasoning.ReasoningResult">
		<rowMap var="s" varType="uri" beanProperty="result"/>
	</perRowResultMap>
	
	<select id="reason" resultMap="reasoningResultMap">	
		SELECT DISTINCT ?s
		
		WHERE 
		{
		<![CDATA[
   			?s #{predicate} <#{object}> .
   		]]>
		} 
	</select>
</sparqlMap>
